# Roadmap auto-sync configuration
# Copy to .env.roadmap.local and complete values.

# Mode 1: Endpoint (Next.js API route)
ROADMAP_AUTOSYNC_URL=https://www.urbanfixar.com/api/admin/roadmap/auto-sync
ROADMAP_AUTOSYNC_TOKEN=replace_with_secure_token

# Mode 2: Direct Supabase fallback (works without endpoint/token)
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=replace_with_service_role_key

# Optional
ROADMAP_AUTOSYNC_BASE_REF=origin/master
