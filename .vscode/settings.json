{
  "css.validate": false,
  "scss.validate": false,
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "url": "https://json.schemastore.org/package"
    }
  ],
  "files.associations": {
    "*.sql": "sql",
    "**/apps/web/lib/supabase/migrations/*.sql": "plaintext",
    "**/supabase/migrations/*.sql": "plaintext",
    "**/node_modules/**/*.sql": "plaintext"
  },
  "mssql.intelliSense.enableErrorChecking": false,
  "mssql.intelliSense.enableIntelliSense": false,
  "mssql.intelliSense.enableSuggestions": false
}
